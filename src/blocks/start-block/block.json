{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "schmitzoide/block-01",
	"version": "0.1.0",
	"title": "Block 01",
	"category": "widgets",
	"icon": "smiley",
	"description": "Client wants a simple block that allows them to display a message on the frontend with a special border around it.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"content": {
			"type": "string",
			"default": "Hello, World!"
		},
		"borderColor": {
			"type": "string",
			"default": "#000000"
		}
	},
	"textdomain": "codeable-skills-chat-gutenberg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
